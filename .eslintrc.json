{
    "extends": "airbnb-base",
    "rules": {
      "array-callback-return": [0],
      "consistent-return": [0],
      "func-names": [0],
      "no-param-reassign": [0],
      "no-restricted-syntax": [0],
      "no-unused-expressions": [0],
      "no-unused-vars": [0],
      "no-use-before-define": [0]
    },
    "globals": {
      "document": false,
      "fetch": false,
      "describe": false,
      "before": false,
      "beforeEach": false,
      "it": false,
      "should": false
    }
}